{
  "name": "04-githooks",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "fix": "pnpm run --silent --parallel '/(lint:fix|format)/'",
    "format": "prettier --write --ignore-unknown --log-level=warn '**/!(*.min|*-lock).{js,mjs,cjs,jsx,ts,tsx,html,css,scss,sass,less,json,jsonc,yaml,yml,graphql,gql}'",
    "lint": "pnpm run --silent --parallel '/lint:(?!.*fix).+$/'",
    "lint:fix": "pnpm run --silent --parallel '/lint:.*:fix$/'",
    "lint:es": "eslint . --report-unused-disable-directives --max-warnings=0",
    "lint:es:fix": "eslint . --fix --report-unused-disable-directives --max-warnings=0",
    "lint:style": "stylelint 'src/**/*.{css,scss,sass,less}' --max-warnings=0",
    "lint:style:fix": "stylelint 'src/**/*.{css,scss,sass,less}' --fix --max-warnings=0",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^9.1.1",
    "@stylistic/eslint-plugin": "^2.1.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "globals": "^15.1.0",
    "prettier": "^3.2.5",
    "stylelint": "^16.5.0",
    "stylelint-config-recess-order": "^5.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.8.0",
    "vite": "^5.2.0"
  }
}
